{
	"name": "react-native-starter",
	"version": "1.0.0",
	"author": {
		"name": "Khanate",
		"email": "dev@khanate.pk",
		"url": "https://khanate.pk"
	},
	"license": "MIT",
	"main": "expo-router/entry",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"update-deps": "yarn upgrade-interactive && expo install --fix && yarn dlx expo-doctor",
		"test": "jest --watch --coverage=false --changedSince=origin/main",
		"test:all": "jest",
		"lint": "eslint . --fix --report-unused-disable-directives && pnpm format && pnpm spellcheck",
		"format": "prettier --write --log-level silent .",
		"spellcheck": "cspell . --quiet",
		"generate-icons": "node ./scripts/generate-icons.ts",
		"env:new": "dotenv-vault new",
		"env:login": "dotenv-vault login",
		"env:push": "yarn env:push:development && yarn env:push:production",
		"env:push:development": "dotenv-vault push development .env.development",
		"env:push:production": "dotenv-vault push production .env.production",
		"env:pull": "yarn env:pull:example && yarn env:pull:development && yarn env:pull:production",
		"env:pull:example": "dotenv-vault pull example .env.example",
		"env:pull:development": "dotenv-vault pull development .env.development",
		"env:pull:production": "dotenv-vault pull production .env.production",
		"env:open": "dotenv-vault open",
		"diag": "tsc --extendedDiagnostics",
		"trace": "tsc --generateTrace ./trace && npx @typescript/analyze-trace trace"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "1.21.0",
		"@sentry/react-native": "5.16.0",
		"expo": "^50.0.2",
		"expo-application": "~5.8.3",
		"expo-av": "~13.10.3",
		"expo-constants": "~15.4.5",
		"expo-device": "~5.9.3",
		"expo-font": "~11.10.2",
		"expo-linear-gradient": "~12.7.0",
		"expo-linking": "~6.2.2",
		"expo-network": "~5.8.0",
		"expo-router": "~3.4.4",
		"expo-secure-store": "~12.8.1",
		"expo-status-bar": "~1.11.1",
		"expo-system-ui": "~2.9.3",
		"expo-updates": "~0.24.8",
		"react": "18.2.0",
		"react-native": "0.73.2",
		"react-native-gesture-handler": "~2.14.1",
		"react-native-paper": "^5.12.1",
		"react-native-paper-dates": "^0.21.7",
		"react-native-reanimated": "~3.6.1",
		"react-native-safe-area-context": "4.8.2",
		"react-native-screens": "~3.29.0",
		"react-native-svg": "14.1.0",
		"sentry-expo": "~7.0.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@babel/core": "^7.23.7",
		"@expo/config": "~8.5.4",
		"@testing-library/jest-native": "^5.4.3",
		"@testing-library/react-native": "^12.4.3",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/babel__core": "^7.20.5",
		"@types/eslint": "^8.56.2",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.11.5",
		"@types/react": "~18.2.48",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"@typescript/analyze-trace": "^0.10.1",
		"cspell": "^8.3.2",
		"dotenv-vault": "^1.25.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-jsx-expressions": "^1.3.2",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-testing-library": "^6.2.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"eslint-plugin-vitest": "^0.3.20",
		"jest": "^29.7.0",
		"jest-expo": "~50.0.1",
		"node-html-parser": "^6.1.12",
		"prettier": "^3.2.4",
		"svgo": "^3.2.0",
		"ts-jest": "^29.1.1",
		"typescript": "^5.3.3"
	}
}
